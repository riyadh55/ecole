

{% block body -%}
    <h1>Reglement</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Datereg</th>
                <td>{{ entity.dateReg|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Naturereg</th>
                <td>{{ entity.natureReg }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reglement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reglement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
